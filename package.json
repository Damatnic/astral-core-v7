{
  "name": "astral-core-v7",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && cross-env NODE_ENV=production next build",
    "build:fast": "next build",
    "build:vercel": "npx prisma generate --no-engine && cross-env NODE_ENV=production SKIP_TYPE_CHECK=true next build",
    "prebuild": "npx prisma generate --no-engine",
    "postinstall": "echo 'Install complete. Run npm run prisma:generate to generate Prisma client.'",
    "prisma:generate": "npx prisma generate",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "vercel:build": "npx prisma generate --no-engine && cross-env NODE_ENV=production SKIP_TYPE_CHECK=true next build",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "migration:preflight": "node scripts/preflight-check.js",
    "migration:run": "node scripts/run-migrations.js",
    "migration:rollback": "node scripts/rollback-migration.js",
    "migration:status": "node scripts/migration-status.js",
    "migration:full": "npm run migration:preflight && npm run migration:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --runInBand --detectOpenHandles",
    "test:unit": "jest --testPathPattern=src/",
    "test:integration": "jest --testPathPattern=tests/integration/",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "optimize:images": "node scripts/optimize-images.js",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production' && !process.env.CI) { try { require('child_process').execSync('npx husky install', {stdio: 'inherit'}); } catch(e) { console.log('Husky not available, skipping install'); } }\"",
    "vercel:env": "node scripts/vercel-env-sync.js",
    "vercel:deploy": "node scripts/vercel-deploy.js",
    "vercel:monitor": "node scripts/vercel-monitor.js",
    "vercel:logs": "vercel logs --follow",
    "vercel:promote": "vercel promote",
    "vercel:rollback": "vercel rollback",
    "vercel:status": "vercel list --count 5",
    "deploy": "npm run vercel:deploy",
    "deploy:prod": "vercel --prod",
    "deploy:preview": "vercel",
    "monitor": "npm run vercel:monitor",
    "perf:analyze": "node scripts/performance-report.js",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./performance-reports/lighthouse-report.html",
    "perf:webvitals": "node scripts/web-vitals-checker.js",
    "perf:full": "npm run perf:analyze && npm run perf:lighthouse",
    "health:check": "curl -f http://localhost:3000/api/health || echo 'Health check failed'",
    "cache:clear": "node -e \"const cache = require('./src/lib/caching/cache-strategies'); cache.cache.invalidatePattern('*');\"",
    "db:optimize": "node scripts/db-optimization.js",
    "security:audit": "npm audit --audit-level high",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "deploy:performance": "npm run perf:full && npm run deploy"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@auth/core": "^0.34.2",
    "@prisma/client": "^6.16.1",
    "@sentry/nextjs": "^8.0.0",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "@types/qrcode": "^1.5.5",
    "@types/sharp": "^0.31.1",
    "@types/speakeasy": "^2.0.10",
    "@types/node": "^20.19.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "typescript": "^5",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "lru-cache": "^11.2.1",
    "lucide-react": "^0.543.0",
    "next": "15.0.3",
    "next-auth": "^4.24.11",
    "prisma": "^6.16.1",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.6.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "stripe": "^18.5.0",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.1.2",
    "@next/bundle-analyzer": "^15.0.3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "cross-env": "^10.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
